  "source": "pragma solidity ^0.8.0;\r\n\r\n\r\nimport \"@openzeppelin/contracts/utils/math/Math.sol\";\r\nimport \"@openzeppelin/contracts/utils/math/SafeMath.sol\";\r\nimport \"./CoreRef.sol\";\r\nimport \"@openzeppelin/contracts/token/ERC20/IERC20.sol\";\r\n\r\n\r\nabstract contract investmentFundPools is CoreRef {\r\n    using SafeMath for uint256;\r\n    uint public lastDepositTime;\r\n    constructor(address core )  CoreRef(core)  {}\r\n    //todo 设置只能来自dcv\r\n\r\n    //将ETH/BTC添加到借贷里面去，生成RUSD\r\n    function saveToDebit() public {}\r\n\r\n    //将RUSD RBT分发到安全池\r\n    function sendTokenSafetyPool(address token,uint amount,address receiver) public {\r\n        IERC20(token).transfer(receiver,amount);\r\n    }\r\n\r\n}\r\n",
